  </dd>
  <dt>
    <tt>hibernate.connection.password</tt>
  </dt>
  <dd>
    <p id="_587e3426-08fb-49e0-952c-f50247c43352">&lt;dbpassword&gt;</p>
  </dd>
  <dt>
    <tt>hibernate.connection.url</tt>
  </dt>
  <dd>
    <p id="_63cec04f-01d2-4671-9dc6-4695fe0bcd05">jdbc:postgresql://&lt;dbhost&gt;:&lt;dbport&gt;/isoreg</p>
  </dd>
</dl>
</clause>
<clause id="_database_parameters_hibernate_properties_xml" inline-header="false" obligation="normative">
